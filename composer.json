{
  "name": "klimser/businessterra",
  "description": "businessterra.uz website",
  "keywords": [
    "businessterra",
    "yii2",
    "framework"
  ],
  "homepage": "http://www.businessterra.uz/",
  "type": "project",
  "license": "GPL-3.0-or-later",
  "support": {
    "issues": "https://businessterra.uz/contacts"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": ">=2.0.6",
    "yiisoft/yii2-bootstrap": "*",
    "yiisoft/yii2-swiftmailer": "*",
    "ilikenwf/nestedSortable": "2.*",
    "yiisoft/yii2-jui": "^2.0",
    "2amigos/yii2-date-picker-widget": "^1.0",
    "kartik-v/yii2-widget-activeform": "^1.4",
    "2amigos/yii2-tinymce-widget": "^1.1",
    "tinify/tinify": "^1.4",
    "kartik-v/yii2-field-range": "^1.3",
    "google/apiclient": "^2.2",
    "himiklab/yii2-recaptcha-widget": "^1.2"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*"
  },
  "config": {
    "process-timeout": 1800
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "ilikenwf/nestedSortable",
        "version": "2.0",
        "source": {
          "type": "git",
          "url": "git@github.com:ilikenwf/nestedSortable.git",
          "reference": "2.0alpha"
        }
      }
    }
  ]
